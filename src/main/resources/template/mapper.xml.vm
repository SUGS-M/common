#set($modulePackage = "")
#if($module)
    #set($modulePackage = ".$module")
#end
<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="${packagePath}${modulePackage}.mapper.${ClassName}Mapper">

    <select id="findByPage" resultType="${packagePath}${modulePackage}.vo.${ClassName}Vo"
            parameterType="${packagePath}${modulePackage}.vo.${ClassName}SearchVo">
        SELECT * FROM ${table.tableName} t WHERE t.del_flag='0'
        #foreach ($column in $columns)
            #if($column.search)
        <if test="$column.ename != null and $column.ename != ''">AND t.$column.sname=#{$column.ename}</if>
            #end
        #end
        <if test="needOrder">ORDER BY t.update_time DESC,t.id</if>
    </select>

</mapper>